import XCTest
@testable import testLexer


class BddAndersenTests: XCTestCase {

    func testNodeDictInit() {
        
        let aNodeDict = NodeDictAndersen()
        
        XCTAssert(aNodeDict.size() == 2)
        XCTAssert(aNodeDict.terminal0() == [], "Answer is \(aNodeDict.terminal0())")
        XCTAssert(aNodeDict.terminal1() == [], "Answer is \(aNodeDict.terminal1())")
    }
  
}
